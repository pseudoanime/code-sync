{
	"Bootstrap row": {
		"prefix": "row",
		"body": [
			"<div class=\"row\">",
			"\t<div class=\"col-xs-${1:12}\">",
			"\t\t$0",
			"\t</div>",
			"</div>"
		],
		"description": "Bootstrap row"
	},
	"Bootstrap column": {
		"prefix": "column",
		"body": [
			"<div class=\"col-xs-${1:12}\">$0</div>",
			"\t<div class=\"col-xs-\">",
			"\t\t$0",
			"\t</div>",
			"</div>"
		],
		"description": "Bootstrap column"
	},
	"Bootstrap input": {
		"prefix": "bstext",
		"body": [
			"<div class=\"form-group\">",
			"\t<label for=\"${1:name}\">${1:name}:</label>",
			"\t<input type=\"text\" id=\"${1:name}\" class=\"form-control\">",
			"</div>"
		],
		"description": "Bootstrap text area"
	},
	"Bootstrap textarea": {
		"prefix": "bstextarea",
		"body": [
			"<div class=\"form-group\">",
			"\t<label for=\"${1:name}\">${1:name}:</label>",
			"\t<textarea name=\"${1:name}\" id=\"${1:name}\" class=\"form-control\"></textarea>",
			"</div>"
		],
		"description": "Bootstrap text area"
	},
	"Bootstrap button": {
		"prefix": "bsbutton",
		"body": [
			"<button type=\"submit\" class=\"btn btn-default\">${1:buttontext}</button>",
		],
		"description": "Bootstrap text area"
	},
	"ng1 radiolist": {
		"prefix": "radiolist",
		"body": [
			"<radio-list ng-attr-list=\"vm.formLookup.${1:list}\" ng-model=\"vm.formData.${2:id}\"",
			"\tid=\"${2:id}\" name=\"${2:id}\" data-inline=\"true\"></radio-list>"
		],
		"description": "Angular 1.x radiolist"
	},
	"ng1 radiolist yesno": {
		"prefix": "yesno",
		"body": [
			"<radio-list ng-attr-list=\"vm.formLookup.YES_NO\" ng-model=\"vm.formData.${1:id}\"",
			"\tid=\"${1:id}\" name=\"${1:id}\" data-inline=\"true\"></radio-list>"
		],
		"description": "Angular 1.x radiolist"
	},
	"ng1 checklist": {
		"prefix": "checklist",
		"body": [
			"<check-list name=\"${1:id}\" class=\"indent-first\" data-inline=\"true\"",
			"\tng-model=\"vm.formData.${1:id}\" data-list=\"vm.formLookup.${2:list}\"></check-list>"
		],
		"description": "Angular 1.x checklist"
	},
	"ng1 datechooser": {
		"prefix": "datechooser",
		"body": [
			"<input type=\"text\" class=\"form-control\" id=\"${1:id}\" ng-attr-value=\"vm.formData.${1:id}\" datechooser />"
		],
		"description": "Angular 1.x datechooser"
	},
	"ng1 datepicker": {
		"prefix": "datepicker",
		"body": [
			"<date-picker ng-model=\"${1:id}\"></date-picker>"
		],
		"description": "Angular 1.x datepicker component"
	},
	"ng1 datelabel": {
		"prefix": "datelabel",
		"body": [
			"<div class=\"form-group\">",
			"\t<label for=\"${1:id}\">$0</label>",
			"\t<date-picker ng-model=\"${1:id}\"></date-picker>",
			"</div>"
		],
		"description": "Angular 1.x datepicker component (w/ form group & label)"
	},
	"ng1 checkbox": {
		"prefix": "checkbox",
		"body": [
			"<input type=\"checkbox\" id=\"${1:id}\" ng-model=\"vm.formData.${1:id}\" ng-checked=\"vm.formData.${1:id}\"/>"
		],
		"description": "Angular 1.x checkbox"
	},
	"ng1 selectlist": {
		"prefix": "selectlist",
		"body": [
			"<select class=\"form-control\" ng-attr-value=\"vm.student.${1:id}\" id=\"${1:id}\" select-list list=\"vm.lookup.${2:list}\"></select>"
		],
		"description": "Angular 1.x selectlist"
	},
	"ng1 label": {
		"prefix": "label",
		"body": [
			"<label for=\"${1:id}\">$0</label>"
		],
		"description": "Angular 1.x label"
	},
	"ng1 sronly label": {
		"prefix": "sronly",
		"body": [
			"<label for=\"${1:id}\" class=\"sr-only-focusable sr-only\">$0</label>"
		],
		"description": "Angular 1.x screen reader label"
	},
	"ng1 formgroup": {
		"prefix": "formgroup",
		"body": [
			"<div class=\"form-group\">",
			"\t<label for=\"${1:id}\">$0</label>",
			"\t<input type=\"text\" class=\"form-control\" id=\"${1:id}\" ng-model=\"vm.formData.${1:id}\" />",
			"</div>"
		],
		"description": "Angular 1.x form group"
	},
	"ng1 formvert": {
		"prefix": "formvert",
		"body": [
			"<div class=\"form-group\">",
			"\t<label class=\"control-label\" for=\"${1:id}\">$0</label>",
			"\t<input type=\"text\" class=\"form-control\" id=\"${1:id}\" ng-model=\"vm.formData.${1:id}\" />",
			"</div>"
		],
		"description": "Angular 1.x vertical form group"
	},
	"ng1 checklabel": {
		"prefix": "checklabel",
		"body": [
			"<label><input type=\"checkbox\" id=\"${1:id}\" ng-model=\"vm.formData.${1:id}\" ng-checked=\"vm.formData.${1:id}\"/>$0</label>"
		],
		"description": "Angular 1.x checkbox with label"
	},
	"ng1 richtext": {
		"prefix": "richtext",
		"body": [
			"<text-angular ta-max-text=\"5000\" ng-model=\"${1:model}\"></text-angular>"
		],
		"description": "Angular 1.x rich text area"
	},
	"ng1 fieldset": {
		"prefix": "cedr-fieldset",
		"body": [
			"<fieldset>",
			"\t<legend>$0</legend>",
			"</fieldset>"
		],
		"description": "Angular 1.x fieldset"
	},
	"ng1 fieldset (disabled)": {
		"prefix": "cedr-fieldset-disabled",
		"body": [
			"<fieldset disabled>",
			"\t<legend>$0</legend>",
			"</fieldset>"
		],
		"description": "Angular 1.x fieldset (disabled)"
	},
	"ng1 studenttag": {
		"prefix": "studenttag",
		"body": [
			"{{vm.student.${1:prop}}}"
		],
		"description": "Display student profile info (Angular 1.x)"
	}
}